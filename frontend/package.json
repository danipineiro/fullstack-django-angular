{
  "name": "angular-docker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "extract:en": "ngx-translate-extract --input \"src/app/**/*.{ts,html}\" --output \"src/assets/i18n/en.json\" --clean --sort --format json --key-as-default-value",
    "extract:rest": "ngx-translate-extract --input \"src/app/**/*.{ts,html}\" --output \"src/assets/i18n/es.json\" --clean --sort --format json --string-as-default-value \"TODO\"",
    "extract-all": "npm run extract:en && npm run extract:rest"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.4.0",
    "@angular/animations": "^19.2.9",
    "@angular/cdk": "^19.2.1",
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/material": "^19.2.1",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/router": "^19.2.9",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.9",
    "@types/jasmine": "~5.1.4",
    "@vendure/ngx-translate-extract": "^9.4.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.5.4"
  }
}
